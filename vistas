alter table empleados
 add constraint FK_empleados_codigocargo
  foreign key (codigocargo)
  references CARGO(ID);
  


select  emplead, count(*)
 from empleados
 group by CC;

  select count(ID)
  from empleados;

  select CC, count(codigosede)
  from empleados 
  group by CC;

  select CC, count(codigoestrato)
  from empleados 
  group by CC;
 
 select max(FECHA_DE_CONTRATACION) as mayor
 from empleados

 select min(FECHA_DE_CONTRATACION) as menor
 from empleados

select  nombres,codigosede,codigocargo
  from empleados
  where CC=10662101;

DELETE FROM empleados 
    WHERE CC=10188530;

select * from empleados
  where FECHA_DE_CONTRATACION between '01/01/00' and '31/12/04';

select nombres,
  max(edad) as mayor,
  min(edad) as menor
  from empleados
  group by nombres;  

select * from empleados
  where edad between 17 and 25;

  select avg(edad)
  from empleados 

   select sum(sueldo)
  from empleados;

select  max(sueldo)
 from empleados;
